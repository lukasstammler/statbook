r <- round(cor(running$kmh, running$kg59), 3)
ggplot(running, aes(x = kmh, y = kg59)) +
geom_point(color = COL[1], size = 3) +
# geom_smooth(method = "lm", se = FALSE, color = "darkgrey") +
